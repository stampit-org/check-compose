{
  "name": "check-compose",
  "version": "4.0.0",
  "description": "Command line tool to test your 'compose' function implementation",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/ bin/",
    "test": "npm run lint && npm run build && ./bin/check-compose stamp-specification",
    "prepublish": "npm run test",
    "coverage": "nyc ./bin/check-compose stamp-specification"
  },
  "bin": {
    "check-compose": "./bin/check-compose"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stampit-org/check-compose.git"
  },
  "keywords": [
    "stamp",
    "composable",
    "compose",
    "object",
    "oop",
    "factory"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stampit-org/check-compose/issues"
  },
  "homepage": "https://github.com/stampit-org/check-compose#readme",
  "devDependencies": {
    "eslint": "^6.4.0",
    "nyc": "^14.1.1",
    "stamp-specification": "^1.5.1"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "tape": "^4.2.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018
    }
  }
}
